  <visual>
    <origin xyz="0 0 0" rpy="0 0 0"/>
    <geometry>
      <!-- 大型相机本体 -->
      <box size="0.04 0.04 0.03"/>
    </geometry>
    <material name="camera_black">
      <color rgba="0.15 0.15 0.15 1"/>
    </material>
  </visual>
  
  <!-- 大型镜头部分 -->
  <visual>
    <origin xyz="0.02 0 0" rpy="0 0 0"/>
    <geometry>
      <!-- 大镜头 -->
      <cylinder length="0.01" radius="0.015"/>
    </geometry>
    <material name="lens_color">
      <color rgba="0.2 0.2 0.9 0.9"/>
    </material>
  </visual>
  
  <!-- 辅助镜头 -->
  <visual>
    <origin xyz="0.02 0.01 0.008" rpy="0 0 0"/>
    <geometry>
      <sphere radius="0.003"/>
    </geometry>
    <material name="sensor_color">
      <color rgba="0.9 0.2 0.2 1"/>
    </material>
  </visual>
  
  <visual>
    <origin xyz="0.02 -0.01 0.008" rpy="0 0 0"/>
    <geometry>
      <sphere radius="0.003"/>
    </geometry>
    <material name="sensor_color"/>
  </visual>
  
  <collision>
    <origin xyz="0 0 0" rpy="0 0 0"/>
    <geometry>
      <box size="0.04 0.04 0.03"/>
    </geometry>
  </collision>
  
  <inertial>
    <origin xyz="0 0 0" rpy="0 0 0"/>
    <mass value="0.08"/>
    <inertia ixx="2e-7" ixy="0" ixz="0" iyy="2e-7" iyz="0" izz="2e-7"/>
  </inertial>
  
  <!-- 在right_hand_rgb_camera链接中添加gazebo标签 -->
<gazebo reference="right_hand_rgb_camera">
  <!-- 设置为静态传感器（减少计算） -->
  <static>true</static>
  
  <!-- 相机传感器 -->
  <sensor type="camera" name="right_hand_camera">
    <update_rate>30.0</update_rate>
    <camera>
      <horizontal_fov>1.3962634</horizontal_fov>  <!-- 80度视野 -->
      <image>
        <width>640</width>
        <height>480</height>
        <format>R8G8B8</format>
      </image>
      <clip>
        <near>0.02</near>
        <far>100</far>
      </clip>
    </camera>
    
    <!-- ROS插件 -->
    <plugin name="camera_controller" filename="libgazebo_ros_camera.so">
      <alwaysOn>true</alwaysOn>
      <updateRate>30.0</updateRate>
      <cameraName>/g1/right_hand_camera</cameraName>
      <imageTopicName>image_raw</imageTopicName>
      <cameraInfoTopicName>camera_info</cameraInfoTopicName>
      <frameName>right_hand_rgb_camera</frameName>
      <hackBaseline>0.07</hackBaseline>
    </plugin>
  </sensor>
</gazebo>
</link>

<!-- 相机安装在平台上，向下倾斜20度 -->
<joint name="right_hand_camera_joint" type="fixed">
  <origin xyz="0 0 0.005" rpy="0 -0.3491 0"/>
  <parent link="right_hand_camera_platform"/>
  <child link="right_hand_rgb_camera"/>
</joint>

<!-- 4. 相机光学帧 -->
<link name="right_hand_rgb_camera_optical"/>

<joint name="right_hand_camera_optical_joint" type="fixed">
  <!-- ROS相机坐标标准：X右，Y下，Z前 -->
  <origin xyz="0 0 0" rpy="-1.57079632679 0 -1.57079632679"/>
  <parent link="right_hand_rgb_camera"/>
  <child link="right_hand_rgb_camera_optical"/>
</joint>

<!-- 5. 保护框/外壳 -->
<link name="right_hand_camera_protection">
  <visual>
    <origin xyz="0 0 0" rpy="0 0 0"/>
    <geometry>
      <!-- 大型保护框 -->
      <box size="0.05 0.05 0.04"/>
    </geometry>
    <material name="protection_color">
      <color rgba="0.7 0.7 0.7 0.4"/>
    </material>
  </visual>
  
  <collision>
    <origin xyz="0 0 0" rpy="0 0 0"/>
    <geometry>
      <box size="0.05 0.05 0.04"/>
    </geometry>
  </collision>
  
  <inertial>
    <origin xyz="0 0 0" rpy="0 0 0"/>
    <mass value="0.03"/>
    <inertia ixx="5e-8" ixy="0" ixz="0" iyy="5e-8" iyz="0" izz="5e-8"/>
  </inertial>
</link>

<joint name="camera_protection_joint" type="fixed">
  <origin xyz="0 0 0.005" rpy="0 0 0"/>
  <parent link="right_hand_camera_platform"/>
  <child link="right_hand_camera_protection"/>
</joint>

<!-- 6. 可选：添加散热片细节 -->
<link name="camera_heat_sink">
  <visual>
    <origin xyz="-0.015 0 0" rpy="0 0 0"/>
    <geometry>
      <box size="0.01 0.04 0.03"/>
    </geometry>
    <material name="heat_sink_color">
      <color rgba="0.9 0.6 0.2 1"/>
    </material>
  </visual>
  
  <inertial>
    <origin xyz="0 0 0" rpy="0 0 0"/>
    <mass value="0.02"/>
    <inertia ixx="1e-8" ixy="0" ixz="0" iyy="1e-8" iyz="0" izz="1e-8"/>
  </inertial>
</link>

<joint name="heat_sink_joint" type="fixed">
  <origin xyz="0 0 0" rpy="0 0 0"/>
  <parent link="right_hand_rgb_camera"/>
  <child link="camera_heat_sink"/>
</joint>










  

</robot>